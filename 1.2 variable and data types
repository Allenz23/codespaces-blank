public class Main
{
    public static void main(String[] args) 
    {
        int a = 5;
        final double b = 5.5;
        System.out.print(a+b);
        boolean c = true;
        String d;
        d = "false";
        a = a-2;
        System.out.println(" = "+(a+b));
        System.out.print(c+" or "+d);
    }
}
